; Engine configuration file.
; It's best edited using the Godot editor.
; If you edit it manually, mind the syntax.
; Visit https://docs.godotengine.org/ for more information.

config_version=5

[application]
config/name="Vibe Rally"
config/icon="res://assets/icon.svg"
run/main_scene="res://scenes/main.tscn"
run/max_fps=120

[display]
window/size/width=1280
window/size/height=720
window/size/resizable=true
window/size/test_width=960
window/size/test_height=540
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[editor]
export/convert_text_resources_to_binary=true

[rendering]
anti_aliasing/quality/msaa_2d=1
anti_aliasing/quality/msaa_3d=1
environment/defaults/default_clear_color=Color(0.082353, 0.101961, 0.152941, 1)
